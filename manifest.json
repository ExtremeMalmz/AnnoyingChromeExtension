{
  "manifest_version": 2,
  "name": "Hello Chrome Extension World",
  "description": "Chrome extension workshop for Le Wagon Tokyo",
  "author": "Eric Malmstrom",
  "version": "1",
  "permissions": ["tabs"],
  

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js" : ["scripts/cheesify.js"]
    }
  ],
  
  "browser_action" : {
    "default_popup" : "popup.html",
    "default_title" : "Erics Chrome Extension"
  }
  
} 